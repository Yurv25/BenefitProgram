<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.student.benefitprogram.ChildDisability"
    android:background="@drawable/cra2">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/income"
        android:layout_marginStart="34dp"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/button2"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Enter Family net income( Max $250000)"
        android:id="@+id/textView3"
        android:layout_marginTop="36dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/income"
        android:textColor="#ffffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calculate"
        android:id="@+id/button2"
        android:layout_marginTop="42dp"
        android:onClick="calcu"
        android:layout_below="@+id/dependant"
        android:layout_centerHorizontal="true"
        android:background="#ffff0705" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dependant"
        android:layout_below="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_marginTop="30dp"
        android:layout_toStartOf="@+id/button2"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Dependants (1 to 3)"
        android:id="@+id/textView10"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/income"
        android:layout_alignStart="@+id/income"
        android:textColor="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/viewin"
        android:layout_marginTop="33dp"
        android:layout_below="@+id/button2"
        android:layout_alignStart="@+id/dependant"
        android:textColor="#ffff160a" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/viewdep"
        android:layout_alignTop="@+id/viewin"
        android:layout_toEndOf="@+id/button2"
        android:textColor="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Your Income"
        android:id="@+id/textView24"
        android:layout_below="@+id/viewin"
        android:layout_alignStart="@+id/viewin"
        android:layout_marginTop="37dp"
        android:textColor="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="($/MONTH)"
        android:id="@+id/textView25"
        android:textColor="#ffffffff"
        android:layout_alignTop="@+id/textView24"
        android:layout_alignStart="@+id/viewdep" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/crawebsite"
        android:id="@+id/textView29"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textColor="#ffff0507" />

</RelativeLayout>
